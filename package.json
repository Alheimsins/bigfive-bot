  },
  "homepage": "https://github.com/Alheimsins/bigfive-bot#readme",
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "nyc": "13.1.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "botbuilder": "4.1.7",
    "restify": "7.3.0"
